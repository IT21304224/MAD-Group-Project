<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".UpdateDelete"
    tools:ignore="ExtraText">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="72dp"
        android:layout_marginTop="-5dp"
        app:srcCompat="@drawable/image" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="30dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="-50dp"
        app:srcCompat="@drawable/baseline_arrow_back_24" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="205dp"
        android:layout_height="43dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="-25dp"
        android:text="Feedback"
        android:textSize="20sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="43dp"
        android:layout_height="35dp"
        android:layout_marginLeft="350dp"
        android:layout_marginTop="-45dp"
        app:srcCompat="@drawable/baseline_account_circle_24" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="35dp"
        android:layout_height="34dp"
        android:layout_marginLeft="300dp"
        android:layout_marginTop="-33dp"
        app:srcCompat="@drawable/baseline_notifications_none_24" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="30dp"
        android:text="Please help to us to serve you better by taking a couple of minutes."
        android:textAlignment="center"
        android:textColor="#101010"
        android:textSize="16sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView52"
        android:layout_width="71dp"
        android:layout_height="32dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="10dp"
        android:text="Name"
        android:textColor="#E18A0A"
        android:textSize="16sp" />


    <EditText
        android:id="@+id/Eetname"
        android:layout_width="246dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="130dp"
        android:layout_marginTop="-30dp"
        android:hint="Enter name" />

    <TextView
        android:id="@+id/tvtype2"
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="15dp"
        android:text="Feddback"
        android:textColor="#E18A0A"
        android:textSize="16sp" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:orientation="horizontal">

        <RadioGroup
            android:id="@+id/Eradiogrup"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >

            <RadioButton
                android:id="@+id/Eradio1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="Excellent"/>

            <RadioButton
                android:id="@+id/Eradio2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="Neutral"/>

            <RadioButton
                android:id="@+id/Eradio3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="Good"/>

            <RadioButton
                android:id="@+id/Eradio4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="Poor"/>

        </RadioGroup>
    </LinearLayout>

    <TextView
        android:id="@+id/textView14"
        android:layout_width="361dp"
        android:layout_height="44dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="30dp"
        android:text="If you have specific feedback, please write to us..."
        android:textSize="16sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/Ebutton3"
        android:layout_width="109dp"
        android:layout_height="50dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="210dp"
        android:background="@android:color/holo_orange_light"
        tools:text="Update" />


    <Button
        android:id="@+id/Ebtncancle1"
        android:layout_width="109dp"
        android:layout_height="50dp"
        android:layout_marginLeft="220dp"
        android:layout_marginTop="-50dp"
        android:background="@android:color/holo_orange_light"
        android:text="Dalete" />

    <EditText
        android:id="@+id/Eetadditfb"
        android:layout_width="351dp"
        android:layout_height="99dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="-280dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Additional comments..."
        android:textColor="#817E7E" />

    <EditText
        android:id="@+id/Eetemail"
        android:layout_width="322dp"
        android:layout_height="39dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Your email " />


    <EditText
        android:id="@+id/Eetnumber"
        android:layout_width="328dp"
        android:layout_height="41dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Your Number" />

</LinearLayout>